<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
<body class="container">

  <header>
    <%- include('../partials/header'); %>
  </header>

<main>
  <div class="jumbotron">


    <h3>Data Type <%= data.data_type %></h3>

    <ol class="list-group list-group-numbered">
      <h1>Shareholders</h1>
      <% data.company_details.Shareholders.forEach(function(shareholder) { %>
        <li class="list-group-item d-flex justify-content-between align-items-start">

              <div class="ms-2 me-auto">
                <div class="fw-bold">Details</div>
                 <p>Names : <%= shareholder.first_name %>  <%= shareholder.second_name %> <%= shareholder.third_name %></p>
                 <p>Id Number :<%= shareholder.id_number %>
                  <p>Description : <%= shareholder.description %></p>
                  <p>Bussiness : <%= shareholder.business %></p>
              </div>
           
            <div class="ms-2 me-auto">
              <div class="fw-bold">Contact</div>
              <p>Mobile Number :<%= shareholder.mobile_number %></p>
              <p>Address :<%= shareholder.address %>
              <p>Citizenship: <%= shareholder.citizenship %></p>
            </div>

            <div class="ms-2 me-auto">
              <div class="fw-bold">Shares</div>
              <p>number :<%= shareholder.shares_number %></p>
            </div>
      
        </li>
      <% }); %>
     
    </ol>




    <ol class="list-group list-group-numbered">
      <h1>Nominalshares</h1>
      <% data.company_details.Nominalshares.forEach(function(nominalshares) { %>
        <li class="list-group-item d-flex justify-content-between align-items-start">

              <div class="ms-2 me-auto">
                 <p>Number of Shares  :  <%= nominalshares.number_of_shares %> </p>
                 <p>Nominal Value  : <%= nominalshares.nominal_value %>
                  <p>Name : <%= nominalshares.name %></p>
              </div>
           
        </li>
      <% }); %>
     
    </ol>

    <ol class="list-group list-group-numbered">
      <h1>Encumburance</h1>
      <% data.company_details.Encumburance.forEach(function(encumburance) { %>
        <li class="list-group-item d-flex justify-content-between align-items-start">

              <div class="ms-2 me-auto">
                 <p>Description  :  <%= encumburance.description %> </p>
                 <p>Date  : <%= encumburance.date %>
                  <p>Amount Secured :  <%= encumburance.amount_secured %></p>
                  <p>review_notes  : <%= typeof encumburance.review_notes != 'undefined' ? encumburance.review_notes : 'n/a' %></p>
                  
              </div>
           
        </li>
      <% }); %>
     
    </ol>



    <ol class="list-group list-group-numbered">
      <h1>Api Request Details</h1>

        <li class="list-group-item d-flex justify-content-between align-items-start">

              <div class="ms-2 me-auto">
                 <p>Request Ref Number  :  <%= request_details.request_ref_number %> </p>
                 <p>bg_dataset_name  : <%= request_details.bg_dataset_name %>
                  <p>Api Request Type  :  <%= request_details.api_request_type %></p>
                  <p>Module Code  :  <%= request_details.module_code %></p>
                  <p>Registration Number  :  <%= request_details.registration_number %></p>
                  <p>Request date  : <%= request_details.request_date %></p>
                  <p>Package Id   :<%= request_details.package_id %></p>

                  
                  
                  
                  
              </div>
           
        </li>
 
     
    </ol>



  </div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>


</body>
</html>